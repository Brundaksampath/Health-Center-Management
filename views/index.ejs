<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    
    <title>Table</title>
</head>
<body style="background-color:black;">
  <div id="dvContents">
    <table class="table table-dark table-striped table-hover" id="tab">
      <thead>
        <tr>
          <th scope="col">USN</th>
          <th scope="col">NAME</th>
          <th scope="col">BRANCH</th>
          <th scope="col">DATE</th>
          <th scope="col">PROBLEM</th>
          <th scope="col">COMMENTS</th>
        </tr>
      </thead>
      <tbody>
        <% detailedList.forEach(i=>{ %>
          <tr>
              <td><%= i.Usn; %></td>
              <td><%= i.Name; %></td>
              <td><%= i.Branch; %></td>
              <td><%= i.RegDate; %></td>
              <td><%= i.Problem; %></td>
              <td><%= i.MediComments; %></td>
            </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
  <br/>
  <input id="xyz" type="button" value="Print Table" onclick="myApp.printTable()" />
</body>
<script>
  let myApp = new function () {
      this.printTable = function () {
        var link = document.getElementById('xyz');
        link.style.display = 'none'; //or
        // link.style.visibility = 'hidden';
        const el = document.getElementById('xyz');
        setTimeout(() => {
          el.style.display = 'block';
        }, 500);
        window.print();
      }
  }
</script>

</html>